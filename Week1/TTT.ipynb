    "class TicTacToe:\n",
    "    def __init__(self):\n",
    "        self.board = [\" \" for _ in range(9)]\n",
    "        self.current_winner = None\n",
    "        \n",
    "    def print_board(self):\n",
    "        for row in [self.board[i * 3:(i + 1) * 3] for i in range(3)]:\n",
    "            print(\"| \" + \" | \".join(row) + \" |\")"
